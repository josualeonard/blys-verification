  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "react-router-dom": "^5.2.0",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "jest": "^26.6.0",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1",
    "webpack": "^4.44.2"
  },
